<package>
  <description brief="Some test and demo programs for the crops_wp6_manipulator stack.">

     This package contains test and demo programs for the crops_wp6_manipulator.

  </description>
  <author>Tien Thanh Nguyen</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/crops_wp6_arm_test</url>

  <depend package="roscpp"/>
  <depend package="actionlib"/>

  <!-- JointTrajectory message -->
  <depend package="trajectory_msgs"/>

  <!-- FollowJointTrajectory action -->
  <depend package="control_msgs"/>

  <!-- PR2's JointTrajectory action -->
  <depend package="pr2_controllers_msgs"/>

  <!-- JointState message -->
  <depend package="sensor_msgs"/>

  <!-- trajectory filtering-->
  <depend package="trajectory_filter_server"/>
  <depend package="arm_navigation_msgs"/>
</package>

