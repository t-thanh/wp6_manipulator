<?xml version="1.2"?>
<!-- see http://www.ros.org/wiki/move_arm/Tutorials/GettingStarted -->
<launch>

    <param name="/use_sim_time" value="true" />

    <node pkg="planning_environment" name="wall_clock_server" type="fake_time.py" />

  <!-- start crops node -->
  <include file="$(find crops_wp6_arm_common)/launch/crops_wp6_arm.launch" />
  
  <node pkg="rviz" type="rviz" name="rviz_planning_components" args="-d $(find crops_wp6_arm_description)/rviz/crops_wp6_arm_description.vcg" />

</launch>
